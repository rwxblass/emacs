(custom-set-faces
 ;; custom-set-faces was added by Custom.
 ;; If you edit it by hand, you could mess it up, so be careful.
 ;; Your init file should contain only one such instance.
 ;; If there is more than one, they won't work right.
 
 '(default ((t (:background "black" :foreground "green" :slant normal :weight normal :height 120 :width normal))))
 '(bold ((t (:background "black" :foreground "green" :weight bold))))
 '(bold-italic ((t (:background "black" :foreground "red" :weight bold))))
 '(border ((t (:background "black" :foreground "black")))))
